<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.bean.FileuploadMapper">

	<cache type="org.mybatis.caches.ehcache.LoggingEhcache">
	</cache>

	<insert id="addFile" parameterType="Fileupload" useGeneratedKeys="true"
		keyProperty="fid">
		insert into
		file(fuuid,fname,description,path,uid,uptime,downtimes,fweight,todid,togid,touid)
		values(#{fuuid},#{fname},#{description},#{path},#{uid},now(),0,#{fweight},#{todid},#{togid},#{touid})
	</insert>




</mapper>